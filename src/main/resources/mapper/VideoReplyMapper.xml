<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.cctyl.mapper.VideoReplyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="io.github.cctyl.domain.po.VideoReply">
        <id column="id" property="id"/>
        <result column="created_date" property="createdDate"/>
        <result column="last_modified_date" property="lastModifiedDate"/>
        <result column="video_id" property="videoId"/>
        <result column="rpid" property="rpid"/>
        <result column="oid" property="oid"/>
        <result column="mid" property="mid"/>
        <result column="root" property="root"/>
        <result column="parent" property="parent"/>
        <result column="dialog" property="dialog"/>
        <result column="ctime" property="ctime"/>
        <result column="current_level" property="currentLevel"/>
        <result column="vip_type" property="vipType"/>
        <result column="message" property="message"/>
        <result column="sex" property="sex"/>
    </resultMap>





    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        created_date,
        last_modified_date,
        video_id,
        rpid,
        oid,
        mid,
        root,
        parent,
        dialog,
        ctime,
        current_level,
        vip_type,
        message,
        sex
    </sql>




</mapper>
